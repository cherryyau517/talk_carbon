<%= form_with(model: @like, local: true) do |f| %>

  <div class="form-group">
    <%= f.label :log_id %>

    <%= f.text_field :log_id, class: "form-control" %>
  </div>

  <%= f.hidden_field :liker_id, value: @user.id %>

  <%= f.submit "Create like", class: "btn btn-block btn-outline-secondary" %>
<% end %>
